hasNth : [a] -> Integer -> Bool;
hasNth lst n = match lst with {
    [] -> False;
    x :: xs -> True if n == 0 otherwise hasNth xs (n - 1);
};

empty: [a] -> Bool;
empty lst = match lst with {
    [] -> True;
    x :: xs -> False;
};

head : [a] -> a;
head lst = match lst with { x :: xs -> x; };

tail : [a] -> [a];
tail lst = match lst with { x :: xs -> xs; };

oneTwoThree : [Integer];
oneTwoThree = [1, 2, 3];

main : Bool;
main = hasNth oneTwoThree 2 && ~(hasNth oneTwoThree 4); 
